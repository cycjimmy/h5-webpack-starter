{
  "name": "h5-webpack-starter",
  "version": "3.1.0",
  "description": "H5 webpack starter for webpack, es6/7, sass, pug, swiper...",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --config webpack/webpack.dev.config.js --watch --color",
    "lint": "eslint -c .eslintrc 'app/**/*.js'",
    "lint:fix": "eslint --fix -c .eslintrc 'app/**/*.js'",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.build.config.js --progress --color",
    "build:watch": "cross-env NODE_ENV=production webpack --config webpack/webpack.build.config.js --progress --color --watch",
    "build:test_server:watch": "cross-env NODE_ENV=production_test_server webpack --config webpack/webpack.build.config.js --progress --color --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycjimmy/h5-webpack-starter.git"
  },
  "keywords": [
    "h5",
    "webpack",
    "sass",
    "pug"
  ],
  "author": "cycjimmy <cycjimmy@gmail.com> (https://github.com/cycjimmy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cycjimmy/h5-webpack-starter/issues"
  },
  "homepage": "https://github.com/cycjimmy/h5-webpack-starter#readme",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@cycjimmy/awesome-js-funcs": "^2.7.0",
    "@cycjimmy/h5-audio-controls": "^3.4.1",
    "@cycjimmy/h5-pages": "^3.1.1",
    "@cycjimmy/h5-preloader": "^3.0.0",
    "@cycjimmy/mini-xhr": "^4.1.0",
    "@cycjimmy/sass-lib": "^1.0.3",
    "@cycjimmy/swiper-animation": "^4.1.2",
    "@cycjimmy/weixin-share": "^5.1.2",
    "fastclick": "^1.0.6",
    "swiper": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.0",
    "@cycjimmy/config-lib": "^1.8.0",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.3",
    "cssnano": "^5.0.10",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-webpack-plugin": "^3.1.1",
    "expose-loader": "^3.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.0.1",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^2.4.4",
    "node-sass": "^6.0.1",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "pug-loader": "^2.4.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.4.1",
    "worker-loader": "^3.0.8"
  }
}
